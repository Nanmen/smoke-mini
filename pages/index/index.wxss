.container {
  height: 100vh;
  display: flex;
  flex-direction: column;
}
.scan-wrapper {
  padding: 16rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  border-bottom: 2rpx solid #f5f5f5;
}

.scan-outline-icon {
  width: 60rpx;
  height: 60rpx;
  margin-right: 10rpx;
}

.content {
  flex: 1;
}

.empty {
  width: 100%;
  height: 300rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}